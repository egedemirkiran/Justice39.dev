    "next": "12.1.4",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-loading-icons": "^1.1.0",
    "react-typical": "^0.1.3",
    "swr": "^2.0.0",
    "tailwindcss": "^3.0.23",
    "use-lanyard": "1.1.0"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
